                    xmlns:local="clr-namespace:Altkom.IGEXAO.MicroCAD.WPFClient"
                    xmlns:v="clr-namespace:Altkom.IGEXAO.MicroCAD.WPFClient.Views"
                    xmlns:vm="clr-namespace:Altkom.IGEXAO.MicroCAD.ViewModels;assembly=Altkom.IGEXAO.MicroCAD.ViewModels"
                    >

    <DataTemplate DataType="{x:Type vm:SchemaViewModel}">
        <v:SchemaView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:WelcomeViewModel}">
        <v:WelcomeView />
    </DataTemplate>
    
</ResourceDictionary>